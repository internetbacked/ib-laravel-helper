{
    "name": "ihsanberahim/laravel-helper",
    "description": "use laravel helpers in wordpress",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Ihsan Berahim",
            "email": "ihsanberahim@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev":
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": ">=5.6.4"
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
